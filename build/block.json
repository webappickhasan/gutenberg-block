{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/gutenberg-block",
  "version": "0.1.0",
  "title": "Gutenberg Block",
  "category": "widgets",
  "icon": "heart",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "fallbackCurrentYear": {
      "type": "string"
    },
    "showStartingYear": {
      "type": "boolean"
    },
    "startingYear": {
      "type": "string"
    }
  },
  "supports": {
    "color": {
      "background": true,
      "text": true
    },
    "html": false,
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "textdomain": "gutenberg-block",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}